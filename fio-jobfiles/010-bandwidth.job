# the next line tells wekatester to report only bandwidth.
# report bandwidth 
[fio-bandwidth-00]
description='wekatester bandwidth workload'
readwrite=randread
blocksize=1m
iodepth=1
numjobs=32
time_based=1
runtime=20
ramp_time=5
group_reporting=1
ioengine=posixaio
nrfiles=200
filesize=50M
create_serialize=0
direct=1
directory=/mnt/wekatester
filename_format=$jobname/$jobnum/$filenum
