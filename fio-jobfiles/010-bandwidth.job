# the next line tells wekatester to report only bandwidth.
# report bandwidth
[fio-bandwidth-00]
description='wekatester bandwidth workload'
readwrite=randread
blocksize=1Mi
iodepth=1
numjobs=20
time_based=1
runtime=20
group_reporting=1
ioengine=posixaio
nrfiles=200
filesize=50M
create_serialize=0
direct=1
directory=/mnt/wekatester
filename_format=$jobname/$jobnum/$filenum
